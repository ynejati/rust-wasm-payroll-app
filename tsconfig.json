{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "experimentalDecorators": true,
    "noLib": false,
    "jsx": "react",
    "outDir": "dist"
  },
  "exclude": [
    "node_modules",
    "rust",
    "target",
    "bin"
  ],
  "include": [
    "pkg"
  ]
}